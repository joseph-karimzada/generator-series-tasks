{
  "name": "telescoping-series",
  "latex": "\\sum_{n=${n0}}^{\\infty}\\frac{${c}}{(n${pmL(a)})(n${pmL(b)})}",
  "params": {
    "n0": {
      "type": "int",
      "min": 1,
      "max": 3
    },
    "a": {
      "type": "int",
      "min": -5,
      "max": 0
    },
    "b": {
      "type": "int",
      "min": 1,
      "max": 5
    },
    "c": {
      "type": "int",
      "min": 1,
      "max": 10
    }
  },
  "constraints": [
    {
      "params": ["a", "b"],
      "code": "a !== b"
    },
    {
      "params": ["n0", "a"],
      "code": "n0 + a > 0"
    },
    {
      "params": ["n0", "b"],
      "code": "n0 + b > 0"
    },
    {
      "params": ["a", "b", "c"],
      "code": "Number.isInteger(c/(abs(a-b)))"
    },
    {
      "params": ["a", "b"],
      "code": "abs(a-b)<=3"
    }
  ]
}